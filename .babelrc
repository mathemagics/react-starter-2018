{
  "presets": [
    [
      "@babel/preset-env", {
        "shippedProposals": true,
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-react",
  ],
  "plugins": [
    "react-hot-loader/babel",
    [
      "module-resolver", {
        "root": ["./src"]
      }
    ]
  ]
}
